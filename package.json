{
  "name": "binr",
  "version": "0.0.2",
  "description": "Reading binary files with style.",
  "homepage": "https://github.com/ahom/binr",
  "author": {
    "name": "Antoine Hom",
    "email": "antoine.hom@gmail.com"
  },
  "repository": "ahom/binr",
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "keywords": [
    "binr",
    "binary",
    "buffer",
    "data",
    "file",
    "read"
  ],
  "dependencies": {
    "text-encoding": "^0.5.2"
  },
  "devDependencies": {
    "browserify": "^8.1.3",
    "istanbul-harmony": "^0.3.1",
    "mocha": "^2.1.0",
    "jshint": "^2.6.0",
    "jshint-stylish": "^1.0.0"
  },
  "scripts": {
    "test": "npm run-script test-jshint && npm run-script test-unit",
    "test-unit": "node --harmony-generators node_modules/mocha/bin/mocha --harmony-generators --reporter spec",
    "test-jshint": "node --harmony-generators node_modules/jshint/bin/jshint --reporter node_modules/jshint-stylish/stylish.js index.js test lib",
    "test-cov": "node --harmony-generators node_modules/istanbul-harmony/lib/cli cover node_modules/mocha/bin/_mocha -- --reporter spec",
    "test-travis": "npm run-script test-jshint && node --harmony-generators node_modules/istanbul-harmony/lib/cli cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec",
    "browser": "node --harmony-generators node_modules/browserify/bin/cmd index.js > browser.js"
  },
  "engines": {
    "node": ">= 0.12.0"
  }
}
